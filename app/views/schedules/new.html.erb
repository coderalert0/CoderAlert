<%= bootstrap_form_for @form, :url => project_schedules_path(@project) do |f| %>
  <%= f.text_field :name %>
  <%= f.select :frequency, options_for_select(Schedule.frequencies.keys.map { |s| [ t(s, :scope => :frequency), s ] }), { :label => 'Frequency' } %>
  <%= f.datetime_select :start, :order => [:hour, :minute, :day, :month, :year] %>
  <%= f.datetime_select :end, :order => [:hour, :minute, :day, :month, :year] %>

  <% @project.users.each do |user| %>
    <%= user.first_name %></br>
  <% end %>

  <%= submit_tag t(:submit), :class => 'btn btn-success' %>
<% end %>