<h3><%= @project.name %></h3>
<div>
  <%= link_to t(:edit), edit_project_path(@project), class: 'btn btn-light border' %>
  <%= link_to t(:delete), project_path(@project), :method => 'delete', class: 'btn btn-light border', data: {confirm: "Are you sure you want to delete the project #{@project.name}?"} %>
</div>

<div class="mt-4">
  <label><%= t(:key, :scope => :project) %>: </label>
  <%= @project.key %>
</div>


