<h2>Create a Project</h2>

<%= bootstrap_form_for @form, :url => wizard_path, :method => :put do |f| %>
  <%= f.text_field :name, :label_class => 'required' %>
  <%= f.text_field :key, :class => 'col-sm-3' %>
  <%= submit_tag t(:next, :scope => :welcome_wizard), :class => 'btn btn-success' %>

<% end %>
